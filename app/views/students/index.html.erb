<h1>This student page</h1>
<div class="row">
  <div class="col s12">
    <h3 class="center-align"> Student Listing</h3>
  </div>
</div>
<div class="row">
  <%  @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card red lighten-5">
        <div class="card-content black-text">
          <span class="card-title"><%= student.name %> </span>
          <p> <%= student.email %></p>
          <p>Course enrollments: none</p>
        </div>
        <div class="card-action">
          <a href="#">View Profile</a>
        </div>
      </div>
    </div>
  <% end %>
</div>
